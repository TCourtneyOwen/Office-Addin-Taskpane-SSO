  <VersionOverrides xmlns="http://schemas.microsoft.com/office/contentappversionoverrides" xsi:type="VersionOverridesV1_0">
    <WebApplicationInfo>
      <Id>{application GUID here}</Id>
      <Resource>api://localhost:{PORT}/{application GUID here}</Resource>
